 int doUnion(int a[], int n, int b[], int m)  {
        map<int, int> mapping;
        for (int i=0;i<n;i++)
        {
            mapping[a[i]]++;
            
            
        }
        for(int i=0;i<m;i++)
        {
            mapping[b[i]]++;
        }
        return mapping.size();
        
        
     
